{
  "name": "frontend-starter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "local": "REACT_APP_BUILD_ENV=local PORT=3001 yarn start",
    "dev": "REACT_APP_BUILD_ENV=dev yarn start",
    "test": "react-scripts test --env=jsdom",
    "lint": "eslint --max-warnings=0 src/**",
    "lint:fix": "yarn lint --fix",
    "codecheck": "yarn lint && yarn test",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "build-local": "react-scripts build",
    "build": "CI=false REACT_APP_BUILD_ENV=dev react-scripts build",
    "build-prod": "CI=false REACT_APP_BUILD_ENV=prod react-scripts build"
  },
  "dependencies": {
    "@babel/polyfill": "7.7.0",
    "@date-io/date-fns": "1.x",
    "@date-io/moment": "^1.3.13",
    "@material-ui/core": "4.9.6",
    "@material-ui/icons": "4.5.1",
    "@material-ui/lab": "^4.0.0-alpha.46",
    "@material-ui/pickers": "^3.2.10",
    "airtable": "^0.8.1",
    "axios": "0.21.1",
    "axios-mock-adapter": "1.17.0",
    "core-js": "^3.6.4",
    "date-fns": "^2.11.1",
    "ethio-qalendar": "^1.2.3",
    "mobx": "5.15.0",
    "mobx-react": "6.1.4",
    "moment": "^2.24.0",
    "node-polyglot": "2.4.0",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "react-google-recaptcha": "^2.0.1",
